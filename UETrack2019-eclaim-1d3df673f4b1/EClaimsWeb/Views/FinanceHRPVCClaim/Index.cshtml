                <div class="card-body">
                    <div class="row">
                        <div class="form-group col-md-4">
                            <label class="">Requester</label>
                            <select asp-for="UserID" asp-items="Model.Users" class="form-control js-example-basic-single w-100" style="width: 100% !important;">
                                <option value="0">All</option>
                            </select>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="">Facility</label>
                            <select asp-for="FacilityID" asp-items="Model.Facilities" class="form-control js-example-basic-single w-100" style="width: 100% !important;">
                                <option value="0">All</option>
                            </select>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="">Status</label>
                            <select asp-for="StatusID" asp-items="Model.Statuses" class="form-control js-example-basic-single w-100" style="width: 100% !important;">
                                <option value="0">All</option>
                            </select>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md-4">
                            <label for="Code" class="">From Date</label>
                            <div id="datepicker-popup" class="input-group date datepicker" style="width: 100%;">
                                <input asp-for="FromDate" type="text" class="form-control" placeholder="DD/MM/YYYY">
                                <span class="input-group-addon input-group-append border-left">
                                    <span class="mdi mdi-calendar input-group-text"></span>
                                </span>
                            </div>
                        </div>
                        <div class="form-group col-md-4">
                            <label for="Code" class="">To Date</label>
                            <div id="datepicker-popup1" class="input-group date datepicker" style="width: 100%;">
                                <input asp-for="ToDate" type="text" class="form-control" placeholder="DD/MM/YYYY">
                                <span class="input-group-addon input-group-append border-left">
                                    <span class="mdi mdi-calendar input-group-text"></span>
                                </span>
