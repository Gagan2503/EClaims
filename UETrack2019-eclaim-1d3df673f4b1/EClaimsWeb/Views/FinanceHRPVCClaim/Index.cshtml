    <div class="card border-bottom">
        <div class="card-header" role="tab" id="heading-10">
            <h6 class="mb-0">
                <a data-toggle="collapse" href="#collapse-10" aria-expanded="false" aria-controls="collapse-10" class="collapsed filterCollapse">
                    Filters
                </a>
            </h6>
        </div>
        <form class="forms-sample mt-4" id="filterform" asp-controller="FinanceHRPVCClaim" asp-action="Index" method="get">
