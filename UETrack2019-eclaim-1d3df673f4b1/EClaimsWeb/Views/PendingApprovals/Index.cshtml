<div class="row">
    <div class="col-md-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <form class="forms-sample mt-4" id="filterform" asp-controller="PendingApprovals" asp-action="Index" method="get">
                    <div class="row">
                        <div class="col-md-10 grid-margin stretch-card">
                            @foreach (var rep in Model.ReportTypes)
                            {
                                <div class="col-sm-2">
                                    <div class="form-check">
                                        <label class="form-check-label" for="@rep.Value" style="line-height: 0.9 !important">
                                            <input asp-for="ModuleName" type="radio" class="form-check-input"
                                                   id="@rep.Value" 
                                                   value="@rep.Value">
                                            @*<label class="form-check-label" style="line-height: 0.9 !important" for="@rep.Value">@rep.Text</label>*@
                                            @*Mileage*@
                                            @rep.Text
                                        </label>
                                    </div>
                                </div>
                            }
                            @*<div class="col-sm-2">
                                    <div class="form-check">
                                        <label class="form-check-label" style="line-height: 0.9 !important">
                                            <input type="radio" class="form-check-input"
                                                   name="membershipRadios" id="radioExpense"
                                                   value="ExpenseClaim" onclick="loadAll('Expense')">
                                            Expense
                                        </label>
                                    </div>
                                </div>
                                <div class="col-sm-2">
                                    <div class="form-check">
                                        <label class="form-check-label" style="line-height: 0.9 !important">
                                            <input type="radio" class="form-check-input"
                                                   name="membershipRadios" id="radioTelephone"
                                                   value="TelephoneBillClaim" onclick="loadAll('Telephone')">
                                            Telephone Bill
                                        </label>
                                    </div>
                                </div>
                                <div class="col-sm-2">
                                    <div class="form-check">
                                        <label class="form-check-label" style="line-height: 0.9 !important">
                                            <input type="radio" class="form-check-input"
                                                   name="membershipRadios" id="radioPVCheque"
                                                   value="PV-ChequeClaim" onclick="loadAll('PV-Cheque')">
                                            PV-Cheque
                                        </label>
                                    </div>
                                </div>
                                <div class="col-sm-2">
                                    <div class="form-check">
                                        <label class="form-check-label" style="line-height: 0.9 !important">
                                            <input type="radio" class="form-check-input"
                                                   name="membershipRadios" id="radioPVGiro"
                                                   value="PV-GIROClaim" onclick="loadAll('PV-GIRO')">
                                            PV-GIRO
                                        </label>
                                    </div>
                                </div>
                                <div class="col-sm-2">
                                    <div class="form-check">
                                        <label class="form-check-label" style="line-height: 0.9 !important">
                                            <input type="radio" class="form-check-input"
                                                   name="membershipRadios" id="radioHRPVCheque"
                                                   value="HRPV-ChequeClaim" onclick="loadAll('HRPV-Cheque')">
                                            HR PV-Cheque
                                        </label>
                                    </div>
                                </div>
                                <div class="col-sm-2">
                                    <div class="form-check">
                                        <label class="form-check-label" style="line-height: 0.9 !important">
                                            <input type="radio" class="form-check-input"
                                                   name="membershipRadios" id="radioHRPVGiro"
