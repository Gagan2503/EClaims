        var strInvoices = "";
        var Invoiceids = [];

        function CheckBoxChange(args,args1) {
            var exists = false;
            var index = '';
            $.each(Invoiceids, function (i, val) {
                if (args1 == 0) {
                    if (args == val) {
                        exists = true;
                        index = i;
                    }
                }
                else {
                    if (args1 == val) {
                        exists = true;
                        index = i;
                    }
                }
            });


            if (exists) {

                Invoiceids.splice(index, 1);
                strInvoices = Invoiceids.join();
            }
            else {
                if (args1 == 0) {
                    Invoiceids.push(args);
                    strInvoices = Invoiceids.join();
                }
                else {
                    Invoiceids.push(args1);
                    strInvoices = Invoiceids.join();
                }
            }

            $("#hdfIds").val(strInvoices);
/*            alert($("#hdfIds").val());*/
            if ($("#StatusID").val() == 9) {
                $("#showAPExport").hide();
            }
            else {
                $("#showAPExportDownload").hide();
            }
            $('#order-listing2 input[type="checkbox"]:checked').each(function () {
                if ($("#StatusID").val() == 9) {
                    $("#showAPExport").show();
                }
                else {
                    $("#showAPExportDownload").show();
                }
            });
        }

        function loadAll(param) {
            alert(param);
        }

        $('#CheckAll').click(function () {
            if ($('#CheckAll').is(':checked')) {
                $('#order-listing2 tbody tr td input[type=checkbox]').each(function () {
                    $(this).prop('checked', true);
                    Invoiceids.push($(this).val());
                });
                if ($("#StatusID").val() == 9) {
                    $("#showAPExport").show();
                }
                else {
                    $("#showAPExportDownload").show();
                }
            }
            else {
                $('#order-listing2 tbody tr td input[type=checkbox]').each(function () {
                    $(this).prop('checked', false);
                    Invoiceids.pop($(this).val());
                });
                if ($("#StatusID").val() == 9) {
                    $("#showAPExport").hide();
                }
                else {
                    $("#showAPExportDownload").hide();
                }
            }
            strInvoices = Invoiceids.join();

            //$("#showAPExport").show();
            $("#hdfIds").val(strInvoices);

        })

